<%= form_with model: @user do |f| %>
  <p>
    <%= f.label :nome %>
    <%= f.text_field :nome %>
  </p>

  <p>
    <%= f.label :sobrenome %>
    <%= f.text_field :sobrenome %>
  </p>

  <p>
    <%= f.label :image %>
    <%= f.file_field :image, accept: 'image/png, image/gif, image/jpeg' %>
  </p>

  <%= f.submit %>
<% end %>

<%= link_to 'Voltar', root_path %>
