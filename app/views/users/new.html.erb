<%= form_with model: @user do |f| %>
  <p>
    <%= f.label :nome %>
    <%= f.text_field :nome %>
  </p>

  <p>
    <%= f.label :sobrenome %>
    <%= f.text_field :sobrenome %>
  </p>

  <p>
    <%= f.label :images %>
    <%= f.file_field :images, multiple: true %>
  </p>

  <%= f.submit %>
<% end %>

<%= link_to 'Voltar', root_path %>
